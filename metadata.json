{
  "name": "puppet-puppetboard",
  "version": "7.0.3-rc0",
  "author": "Vox Pupuli",
  "summary": "Install and configure PuppetBoard",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-puppetboard.git",
  "project_page": "https://github.com/voxpupuli/puppet-puppetboard",
  "tags": [
    "puppetboard",
    "puppetdb",
    "voxpupuli"
  ],
  "issues_url": "https://github.com/voxpupuli/puppet-puppetboard/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.8 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 7.0.0"
    },
    {
      "name": "puppet/python",
      "version_requirement": ">= 4.1.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 1.3.1 < 4.0.0"
    }
  ]
